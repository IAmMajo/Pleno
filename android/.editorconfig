# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*.{kt,kts}]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

[android/dtos/**]
indent_style = unset
indent_size = unset
end_of_line = unset
charset = unset
